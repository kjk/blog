        "properties": {
          "title": [
            [
              "How does apt know what packages are available?"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "How does apt know what packages are available?"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1531204097272,
        "id": "ce617085-2be7-4358-9b4c-433ff1df15c9",
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1531204097272,
        "parent_id": "75f1199f-2c17-4fb9-87bc-9b1fb4989375",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "Apt queries package servers to get a list of available deb packages. Default Ubuntu installation knows about official Ubuntu servers but you can run your own server to provide additional packages."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Apt queries package servers to get a list of available deb packages. Default Ubuntu installation knows about official Ubuntu servers but you can run your own server to provide additional packages."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1531204097272,
        "id": "25b42e2d-4577-4cd4-aa84-486908153c50",
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1531204097272,
        "parent_id": "75f1199f-2c17-4fb9-87bc-9b1fb4989375",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "List of servers is in "
            ],
            [
              "/etc/apt/sources.list",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ". Here’s how it looks by default on Ubuntu 16.04:"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "List of servers is in "
          },
          {
            "Text": "/etc/apt/sources.list",
            "AttrFlags": 2
          },
          {
            "Text": ". Here’s how it looks by default on Ubuntu 16.04:"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1531204097272,
        "id": "fdd6aa8e-5094-49b7-8f3d-6940b3cfd870",
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1531383341195,
        "parent_id": "75f1199f-2c17-4fb9-87bc-9b1fb4989375",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Shell"
            ]
          ],
          "title": [
            [
              "$ cat /etc/apt/sources.list\ndeb http://archive.ubuntu.com/ubuntu/ xenial main restricted universe multiverse\ndeb http://archive.ubuntu.com/ubuntu/ xenial-updates main restricted universe multiverse\ndeb http://security.ubuntu.com/ubuntu/ xenial-security main restricted universe multiverse"
            ]
          ]
        },
        "type": "code",
        "version": 3,
        "code": "$ cat /etc/apt/sources.list\ndeb http://archive.ubuntu.com/ubuntu/ xenial main restricted universe multiverse\ndeb http://archive.ubuntu.com/ubuntu/ xenial-updates main restricted universe multiverse\ndeb http://security.ubuntu.com/ubuntu/ xenial-security main restricted universe multiverse",
        "code_language": "Shell"
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1531204097272,
        "id": "0ee18d46-9577-4f6d-ad43-4472aa9c669e",
