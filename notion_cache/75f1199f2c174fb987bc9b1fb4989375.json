        "parent_table": "block",
        "properties": {
          "title": [
            [
              "For security, packages are signed with private keys. You need public key to verify package signature."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "For security, packages are signed with private keys. You need public key to verify package signature."
          }
        ]
      },
      {
        "alive": true,
