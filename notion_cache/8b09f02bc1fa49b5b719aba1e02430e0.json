        "properties": {
          "title": [
            [
              "Avoiding complexity"
            ]
          ]
        },
        "type": "sub_header",
        "version": 1,
        "inline_content": [
          {
            "Text": "Avoiding complexity"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1531200907544,
        "id": "c3e1e932-2e1e-4be3-a3a6-e550dc776a9f",
