        "properties": {
          "title": [
            [
              "Good news everyone: a new version of "
            ],
            [
              "SumatraPDF",
              [
                [
                  "a",
                  "https://www.sumatrapdfreader.org/free-pdf-reader.html"
                ]
              ]
            ],
            [
              ", a free, small, fast, open-source PDF viewer for Windows, is ready."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Good news everyone: a new version of "
          },
          {
            "Text": "SumatraPDF",
            "Link": "https://www.sumatrapdfreader.org/free-pdf-reader.html"
          },
          {
            "Text": ", a free, small, fast, open-source PDF viewer for Windows, is ready."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
