          "title": [
            [
              "#if defined(__GNUC__) \u0026\u0026 (__GNUC___ \u003e 5 || (__GNUC__ == 5 \u0026\u0026 __GNUC_MINOR__ \u003e= 1))\n// this is gcc 5.1 or greater\n#endif"
            ]
          ]
