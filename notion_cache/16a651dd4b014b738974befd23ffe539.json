        "parent_table": "block",
        "properties": {
          "title": [
            [
              "bp creates a new breakpoint on entry to a given function. A string "
            ],
            [
              "\"kb; g\"",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " is composed of WinDBG commands executed when a breakpoint is hit. In this case I just dump the callstack with "
            ],
            [
              "kb",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " and continue execution with "
            ],
            [
              "g",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              "."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "bp creates a new breakpoint on entry to a given function. A string "
          },
          {
            "Text": "\"kb; g\"",
            "AttrFlags": 2
          },
          {
            "Text": " is composed of WinDBG commands executed when a breakpoint is hit. In this case I just dump the callstack with "
          },
          {
            "Text": "kb",
            "AttrFlags": 2
          },
          {
            "Text": " and continue execution with "
          },
          {
            "Text": "g",
            "AttrFlags": 2
          },
          {
            "Text": "."
          }
        ]
      },
