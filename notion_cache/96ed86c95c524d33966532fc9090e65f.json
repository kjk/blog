        "parent_table": "block",
        "properties": {
          "title": [
            [
              "Google advertises only one quality of WebP (making web faster) and they focus on a quality that everyone cares about."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Google advertises only one quality of WebP (making web faster) and they focus on a quality that everyone cares about."
