        "parent_id": "96ed86c9-5c52-4d33-9665-32fc9090e65f",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "Both of them failed in the marketplace. It turns out that no one cared enough about those additional features to displace JPEG."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Both of them failed in the marketplace. It turns out that no one cared enough about those additional features to displace JPEG."
          }
        ]
      },
