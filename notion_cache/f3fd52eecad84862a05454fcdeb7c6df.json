        "properties": {
          "title": [
            [
              "You set "
            ],
            [
              "svn:externals",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " property on an existing directory. The value of this property is a list of directory/repository path values, separated by spaces."
            ]
          ]
        },
        "type": "text",
