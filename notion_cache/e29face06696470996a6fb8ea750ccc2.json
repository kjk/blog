        "parent_id": "e29face0-6696-4709-96a6-fb8ea750ccc2",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "NSIS is better but the default is to create a multi-step installer where users has to click and click and click to advance the installation."
            ]
          ]
        },
        "type": "text",
        "version": 225,
        "inline_content": [
          {
            "Text": "NSIS is better but the default is to create a multi-step installer where users has to click and click and click to advance the installation."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1554367440000,
        "id": "34365611-51cf-4385-94d9-27b698f0c2f6",
