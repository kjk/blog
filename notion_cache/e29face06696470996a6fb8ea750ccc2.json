        "parent_id": "e29face0-6696-4709-96a6-fb8ea750ccc2",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "As a result they end up with lowest common denominator for the installation experience."
            ]
          ]
        },
        "type": "text",
        "version": 117,
        "inline_content": [
          {
            "Text": "As a result they end up with lowest common denominator for the installation experience."
          }
