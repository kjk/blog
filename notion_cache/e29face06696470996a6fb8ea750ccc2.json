        "parent_id": "e29face0-6696-4709-96a6-fb8ea750ccc2",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "Installation logic is just few kilobytes of the code so I create applications that are their own installers."
            ]
          ]
        },
        "type": "text",
        "version": 163,
        "inline_content": [
          {
            "Text": "Installation logic is just few kilobytes of the code so I create applications that are their own installers."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1554367740000,
        "id": "6f2a8bf8-f1cb-43e9-9ac5-a1c7be191a69",
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
