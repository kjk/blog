        "parent_id": "751d064a-76bf-46fa-9b29-3da1a88cd033",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "The OS libraries will be frozen, unable to evolve and adapt to new trends."
            ]
          ]
        },
        "type": "text",
        "version": 130,
        "inline_content": [
          {
            "Text": "The OS libraries will be frozen, unable to evolve and adapt to new trends."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1554262860000,
        "id": "b837efd4-1b20-49da-9b0d-fedd7fc53136",
