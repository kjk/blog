        "last_edited_time": 1531195893572,
        "parent_id": "8fd3d2c1-a81f-4509-9c20-02394b356eab",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "It no longer blindly registers itself as a default handler for PDF files. It asks the user nicely first."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "It no longer blindly registers itself as a default handler for PDF files. It asks the user nicely first."
          }
