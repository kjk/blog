        "parent_table": "block",
        "properties": {
          "title": [
            [
              "There is a solution to this problem."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "There is a solution to this problem."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1531195851715,
        "id": "e219eb8a-c1a7-44a3-8f6b-4880d004ecbe",
