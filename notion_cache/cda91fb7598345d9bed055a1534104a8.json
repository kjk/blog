        "properties": {
          "title": [
            [
              "Use "
            ],
            [
              "TransFS",
              [
                [
                  "a",
                  "http://transfs.com/"
                ]
              ]
            ],
            [
              " to find a provider for internet merchant account. Don’t use regular banks for it (like Bank Of America etc.)"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Use "
          },
          {
            "Text": "TransFS",
            "Link": "http://transfs.com/"
          },
          {
            "Text": " to find a provider for internet merchant account. Don’t use regular banks for it (like Bank Of America etc.)"
          }
        ]
