        "properties": {
          "title": [
            [
              "I’ve been recently writing desktop .NET applications in C# and existing (free) options didn’t meet my needs well."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "I’ve been recently writing desktop .NET applications in C# and existing (free) options didn’t meet my needs well."
          }
